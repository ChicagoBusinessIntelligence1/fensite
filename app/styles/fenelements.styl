  color white

.cursor-p
  cursor pointer

.full-height
  height 100%
  color #ffffff
  text-align center
  margin 5px
  box-shadow 10px 10px 5px #888888
  border-radius 15px
  position: relative

/*overflow hidden*/

ul.fen-elements
  margin 0 auto
  ul(style = disc)
  li
    position: relative
    display: inline-block !important
    list-style: none;
    margin-left: auto;
    margin-right: auto;
    width 150px !important
    height 150px !important

.element-info
  position relative
  bottom -100%
  background-color darkseagreen
  height 100%


