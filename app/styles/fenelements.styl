.el1
  background-color red
  size(150px)
  color white
  text-align center
  border-radius 15px
  position relative
  overflow hidden


.el1-more
  @extend .el1
  background-color #808080
  position: relative
  top 150px

.el2
  background-color blue
  size(150px)
  color white
  text-align center
  border-radius 15px
  position relative
  overflow hidden


.el2-more
  @extend .el1
  background-color #808080
  position: relative
  top 150px

.va
  top 50%
  position absolute
  width 100%
  text-align center
  /*z-index -99*/

.green
  background-color green

.blue
  background-color blue

.black
  background-color black
  color white
.cursor-p
  cursor pointer

.full-height
  height 100%
  color #ffffff
  text-align center
  margin 5px
  box-shadow 10px 10px 5px #888888
  border-radius 15px
  position: relative
  /*overflow hidden*/

ul.fen-elements
  margin 0 auto
  ul(style = disc)
  li
    position: relative
    display: inline-block !important
    list-style: none;
    margin-left: auto;
    margin-right: auto;
    width 150px !important
    height 150px !important

.element-info
  position relative
  bottom -100%
  background-color darkseagreen
  height 100%


